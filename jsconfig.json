{
  "compilerOptions": {
    "jsx": "react",
    "baseUrl": ".",
    "paths": {
      "~assets/*": [
        "src/assets/*"
      ],
      "~css/*": [
        "src/css/*"
      ],
      "~font/*": [
        "src/font/*"
      ],
      "~pages/*": [
        "src/pages/*"
      ],
      "~/*": [
        "src/*"
      ],
      "~utils/*": [
        "src/utils/*"
      ],
      "~components/*": [
        "src/components/*"
      ],
      "~apis/*": [
        "src/apis/*"
      ],
      "~hook/*": [
        "src/hook/*"
      ],
      "~Text/*": [
        "src/components/Text/*"
      ]
    }
  }
}